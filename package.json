{
  "name": "konekoya-blog",
  "description": "Konekoya's personal blog",
  "version": "1.0.0",
  "author": "konekoya <konekoya@gmail.com>",
  "keywords": [
    "Blog, Frontend, Web, React"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "prepublish": "yarn lint && yarn build --prefix-paths",
    "clean": "gatsby clean",
    "publish": "gh-pages -d public",
    "start": "npm-run-all --parallel develop lint:watch",
    "lint": "esw src --cache",
    "lint:watch": "yarn lint --watch",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.js\""
  },
  "dependencies": {
    "gatsby": "^2.18.17",
    "gatsby-link": "^2.2.27",
    "gatsby-plugin-react-helmet": "^3.1.18",
    "gatsby-plugin-styled-components": "^3.1.16",
    "gatsby-remark-prismjs": "^3.3.28",
    "gatsby-source-filesystem": "^2.1.43",
    "gatsby-transformer-remark": "^2.6.45",
    "prettier": "^1.19.1",
    "prismjs": "^1.17.1",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^22.4.1",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.6.1",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "eslint-watch": "^3.1.0",
    "gh-pages": "^1.1.0",
    "jest": "22.4.2",
    "npm-run-all": "^4.1.2"
  }
}
