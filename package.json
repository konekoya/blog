{
  "name": "konekoya-blog",
  "description": "Konekoya's personal blog",
  "version": "1.0.0",
  "author": "konekoya <konekoya@gmail.com>",
  "keywords": [
    "Blog, Frontend, Web, React"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "predeploy": "yarn lint && yarn build --prefix-paths",
    "deploy": "gh-pages -d public",
    "start": "npm-run-all --parallel develop lint:watch",
    "lint": "esw src --cache",
    "lint:watch": "yarn lint --watch",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.js\""
  },
  "dependencies": {
    "gatsby": "^1.9.145",
    "gatsby-link": "^1.6.32",
    "gatsby-plugin-react-helmet": "^1.0.8",
    "gatsby-plugin-styled-components": "^2.0.4",
    "gatsby-remark-prismjs": "^1.2.15",
    "gatsby-source-filesystem": "^1.5.11",
    "gatsby-transformer-remark": "^1.7.33",
    "styled-components": "^2.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^22.4.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "2.9.1",
    "enzyme-adapter-react-15.4": "^1.0.5",
    "enzyme-to-json": "3.3.0",
    "eslint": "^4.6.1",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "eslint-watch": "^3.1.0",
    "gh-pages": "^1.1.0",
    "jest": "22.4.2",
    "npm-run-all": "^4.1.2",
    "react-test-renderer": "^16.2.0"
  }
}
