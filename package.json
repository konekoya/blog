{
  "name": "konekoya-blog",
  "description": "Konekoya's personal blog",
  "version": "1.0.0",
  "author": "konekoya <konekoya@gmail.com>",
  "keywords": ["Blog, Frontend, Web, React, Redux"],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "predeploy": "yarn lint && yarn build",
    "develop": "gatsby develop",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "start": "npm-run-all --parallel develop lint:watch",
    "lint": "esw src --cache",
    "lint:watch": "yarn lint --watch",
    "format":
      "prettier --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.js\""
  },
  "dependencies": {
    "gatsby": "^1.9.145",
    "gatsby-link": "^1.6.32",
    "gatsby-plugin-react-helmet": "^1.0.8",
    "gatsby-plugin-styled-components": "^2.0.4",
    "gatsby-source-filesystem": "^1.5.11",
    "gatsby-transformer-remark": "^1.7.26",
    "styled-components": "^2.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "enzyme": "3.3.0",
    "enzyme-to-json": "3.3.0",
    "eslint": "^4.6.1",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "eslint-watch": "^3.1.0",
    "gh-pages": "^1.1.0",
    "jest": "20.0.4",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.9.2"
  }
}
